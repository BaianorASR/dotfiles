{
  //##########FONT##############
  "editor.fontFamily": "JetBrainsMono Nerd Font",
  "editor.fontSize": 15,
  "editor.letterSpacing": 0.1,
  "editor.fontLigatures": true,
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font Mono",
  "terminal.integrated.fontSize": 15,
  "editor.wordWrap": "on",
  "editor.minimap.enabled": false,
  // ############CODE RUNER##########
  "code-runner.clearPreviousOutput": true,
  // ############################
  "cSpell.language": "en,pt,pt_BR",
  "tabnine.experimentalAutoImports": true,
  "material-icon-theme.folders.theme": "specific",
  "material-icon-theme.folders.color": "#90a4ae",
  "editor.tabSize": 2,
  "editor.bracketPairColorization.enabled": true,
  "security.workspace.trust.untrustedFiles": "open",
  // ##############################################
  //                  Pretier
  // ##############################################
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.singleQuote": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "eslint.lintTask.enable": true,

  "material-icon-theme.saturation": 0.6,
  "material-icon-theme.activeIconPack": "react_redux",
  "markdown.preview.doubleClickToSwitchToEditor": false,
  "terminal.external.linuxExec": "kitty",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "material-icon-theme.hidesExplorerArrows": false,
  "workbench.iconTheme": "material-icon-theme",
  "git.autofetch": true,
  "vsintellicode.features.apiExamples": "enabled",
  "vsintellicode.features.python.deepLearning": "enabled",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "workbench.productIconTheme": "material-product-icons",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "workbench.colorTheme": "Nord",
  "liveshare.presence": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "stylelint.packageManager": "yarn",
  "cSpell.userWords": [
    "baianor",
    "Baianor",
    "camelcase",
    "inpt",
    "nord",
    "reduxjs",
    "tailwindcss",
    "testid",
    "Trybe"
  ],
  "importCost.smallPackageColor": "#EBCB8B",
  "importCost.mediumPackageColor": "#D08770",
  "importCost.largePackageColor": "#BF616A",
  // "[javascript]": {
  //   "editor.formatOnSave": true
  // },
  "editor.formatOnSave": true,
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "eslint.packageManager": "yarn",
  "intellicodeCompletions.language.typescript": true,
  "intellicodeCompletions.language.javascript": true,
  "prettier.trailingComma": "all",
  "prettier.withNodeModules": true,
  "material-icon-theme.opacity": 1,
  "javascript.format.semicolons": "insert",
  "workbench.tree.indent": 6,
  "workbench.tree.renderIndentGuides": "none",
  "emmet.triggerExpansionOnTab": true,
  "window.customMenuBarAltFocus": false,
  "window.enableMenuBarMnemonics": false,
  "autoimport.showNotifications": true,
  "colorize.languages": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "json",
    "html",
    "css",
    "sass",
    "scss",
    "less",
    "postcss",
    "sss",
    "stylus",
    "xml",
    "svg"
  ],
  "autoimport.filesToScan": "./src/**",
  "prettier.printWidth": 90,
  "eslint.codeAction.disableRuleComment": {
    "enable": true,
    "location": "sameLine"
  },
  "reactSnippets.settings.prettierEnabled": true,
  "[javascript]": {
    "editor.defaultFormatter": "ms-vsliveshare.vsliveshare"
  },
  "typescriptHero.imports.organizeOnSave": true,
  "typescriptHero.imports.disableImportsSorting": true,
  "typescriptHero.imports.disableImportRemovalOnOrganize": true
}
